{
  "name": "exos-cli",
  "version": "0.1.0",
  "description": "CLI tool for React projects",
  "main": "./dist/exos-cli.js",
  "types": "./dist/types/exos-cli.d.ts",
  "bin": {
    "exos-cli": "./dist/exos-cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "build": "rm -rf dist && tsc && cp -r ./src/commands/create/templates ./dist/commands/create/templates",
    "start": "node .",
    "debug": "node --nolazy --inspect-brk=9229 ./dist/exos-cli.js create",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nanovazquez/exos.git"
  },
  "keywords": [
    "react",
    "cli",
    "react-cli",
    "React CLI",
    "exos"
  ],
  "author": "nanovazquez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nanovazquez/exos/issues"
  },
  "homepage": "https://github.com/nanovazquez/exos#readme",
  "dependencies": {
    "fs-extra": "^9.0.0",
    "yargs-interactive": "^3.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/fs-extra": "^8.1.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.5",
    "chai": "^4.2.0",
    "coveralls": "^3.0.11",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
}
