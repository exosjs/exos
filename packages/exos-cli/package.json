{
  "name": "exos-cli",
  "version": "1.0.0",
  "description": "CLI tool for React projects",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "build": "rm -rf lib && tsc",
    "start": "node ./lib/src/exos-cli.js",
    "debug": "npm run build && node --nolazy --inspect-brk=9229 ./lib/src/exos-cli.js"
  },
  "main": "./lib/src/exos-cli.js",
  "types": "lib/types/index.d.ts",
  "bin": {
    "exos-cli": "./lib/src/exos-cli.js"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nanovazquez/exos.git"
  },
  "keywords": [
    "react",
    "cli",
    "react-cli",
    "React CLI",
    "exos"
  ],
  "author": "nanovazquez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nanovazquez/exos/issues"
  },
  "homepage": "https://github.com/nanovazquez/exos#readme",
  "dependencies": {
    "yargs-interactive": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^10.11.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.1"
  }
}
