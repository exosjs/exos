{
  "name": "exos-cli",
  "version": "0.0.4",
  "description": "CLI tool for React projects",
  "main": "./lib/src/exos-cli.js",
  "types": "lib/types/src/exos-cli.d.ts",
  "bin": {
    "exos-cli": "./lib/src/exos-cli.js"
  },
  "folders": [
    "lib"
  ],
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "build": "rm -rf lib && tsc && cp -r src/commands/create/templates lib/src/commands/create/templates",
    "start": "node ./lib/src/exos-cli.js",
    "debug": "node --nolazy --inspect-brk=9229 ./lib/src/exos-cli.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nanovazquez/exos.git"
  },
  "keywords": [
    "react",
    "cli",
    "react-cli",
    "React CLI",
    "exos"
  ],
  "author": "nanovazquez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nanovazquez/exos/issues"
  },
  "homepage": "https://github.com/nanovazquez/exos#readme",
  "dependencies": {
    "yargs-interactive": "^2.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/fs-extra": "^5.0.5",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.4",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "fs-extra": "^7.0.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.1"
  }
}
